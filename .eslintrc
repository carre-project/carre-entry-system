{
  "extends": "eslint:recommended",
  "rules":{
    "no-mixed-spaces-and-tabs":0,
    "angular/log":0,
    "no-console":0,
    "angular/controller-name":0
  },
  "plugins": ["angular"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true
  }
}
